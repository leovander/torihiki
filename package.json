{
  "name": "torihiki",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node ./dist/index.js",
    "serve": "pm2 start dist/index.js --name torihiki",
    "build": "tsc",
    "watch": "concurrently \"npm:watch-*\"",
    "watch-build": "tsc -w",
    "watch-nodemon": "nodemon ./dist/index.js",
    "ts-watch-nodemon": "nodemon ./src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prettier": "npx prettier . --write",
    "lint": "npx eslint",
    "prepare": "husky",
    "generate-licenses": "PACKAGE_NAME=torihiki node tools/generate-licenses.mjs"
  },
  "author": "leovander",
  "license": "AGPL-3.0-only",
  "description": "Torihiki (とりひき), a Telegram and Discord-Self bot, that forwards Discord messages to Telegram.",
  "dependencies": {
    "@azure/msal-node": "^3.8.4",
    "@bull-board/api": "^5.18.3",
    "@bull-board/express": "^5.18.3",
    "@telegraf/session": "2.0.0-beta.7",
    "axios": "^1.7.2",
    "bullmq": "^5.7.8",
    "discord.js-selfbot-v13": "^3.1.4",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "imap": "^0.8.19",
    "ioredis": "^5.4.1",
    "mailparser": "^3.9.1",
    "redis": "^4.6.13",
    "rss-to-json": "^2.1.1",
    "telegraf": "^4.16.3",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.3.0",
    "@types/express": "^4.17.21",
    "@types/imap": "^0.8.43",
    "@types/mailparser": "^3.4.6",
    "@types/node": "^20.12.11",
    "@types/redis-info": "^3.0.3",
    "concurrently": "^8.2.2",
    "eslint": "^9.3.0",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.3.0",
    "husky": "^9.0.11",
    "license-checker": "^25.0.1",
    "lint-staged": "^15.2.4",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "tsc-watch": "^6.2.0",
    "typegram": "^5.2.0",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.10.0"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write --ignore-unknown",
      "eslint"
    ]
  },
  "packageManager": "pnpm@8.6.0+sha1.71f9126a20cd3d00fa47c188f956918858180e54"
}
